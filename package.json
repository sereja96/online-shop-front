{
  "name": "online-shop-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:css": "node-sass --output-style compressed -o ./dist ./src/scss/app.scss",
    "build:js": "webpack ./src/index.js ./dist/bundle.js",
    "build:all": "npm run build:css && npm run build:js",
    "watch:all": "npm-scripts-watcher",
    "run-server": "npm run build:all && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "parallelshell 'npm run run-server' 'npm run watch:all'"
  },
  "watch": {
    "src/scss/**/*.scss": [
      "build:css"
    ],
    "src/**/*.js": [
      "build:js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "null"
  },
  "author": "sereja",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.2",
    "material-ui": "^0.16.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-router": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "node-sass": "^3.4.2",
    "npm-scripts-watcher": "^1.0.2",
    "parallelshell": "^2.0.0",
    "path": "^0.12.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack-dev-server": "^1.16.2"
  }
}
